namespace shuttermsg;

enum Version : uint8 {
    Initial
}

union CryptoParam {
    None,
    NaClSecretBox
}

table None {
}

table NaClSecretBox {
    session:uint8;
    nonce:[uint8];
    mac:[uint8];
}

table Shutterheader {
    version:Version;
    crypt:CryptoParam;
    payload:[uint8];
}

root_type Shutterheader;
